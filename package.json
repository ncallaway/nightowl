{
  "name": "nightowl-cli",
  "version": "0.1.7",
  "description": "a cli for managing and issuing a collection of curl requests",
  "main": "index.js",
  "scripts": {
    "dev": "npx tsc --watch",
    "build": "npx tsc",
    "check": "npx tsc --noEmit",
    "lint": "npx eslint src --max-warnings=0",
    "test": "npx jest",
    "test:coverage": "npx jest --coverage --coverage-directory='coverage' && echo '\nopen ./coverage/lcov-report/index.html'",
    "integration": "bin/integration.sh",
    "prepare": "husky install",
    "g:migration": "MIGRATIONS_DIR=src/migrations npx knex migrate:make -x ts"
  },
  "bin": {
    "owl": "dist/nightowl.js",
    "nightowl": "dist/nightowl.js"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@types/chalk": "^2.2.0",
    "@types/command-line-args": "^5.2.0",
    "@types/conf": "^3.0.0",
    "@types/hawk": "^9.0.2",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.175",
    "@types/mime-types": "^2.1.1",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^16.10.3",
    "@types/nunjucks": "^3.2.0",
    "@types/pg": "^8.6.4",
    "@types/prompts": "^2.0.14",
    "@types/signale": "^1.4.2",
    "@types/sqlite3": "^3.1.7",
    "@types/tough-cookie": "^2.3.3",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-jest": "^27.2.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "jest": "^27.2.4",
    "prettier": "^2.4.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "author": "Noah Callaway",
  "license": "UNLICENSED",
  "dependencies": {
    "@js-temporal/polyfill": "^0.3.0",
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.1",
    "command-line-args": "^5.2.0",
    "conf": "git://github.com/ncallaway/conf.git#699698c09eea55413e80d3e0dccd6cdca57fcdae",
    "env-paths": "^ 2.2.1",
    "hawk": "^9.0.0",
    "jwt-authentication": "^0.4.0",
    "knex": "^0.95.14",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.34",
    "mkdirp": "^1.0.4",
    "neverthrow": "^4.2.2",
    "node-libcurl": "^2.3.3",
    "nunjucks": "^3.2.3",
    "pg": "^8.7.1",
    "pg-types": "^3.0.1",
    "prompts": "^2.4.2",
    "signale": "^1.4.0",
    "sqlite3": "^5.0.2",
    "tough-cookie": "^2.3.3",
    "uuid": "^8.3.2"
  }
}
